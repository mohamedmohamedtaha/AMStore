<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.imagine.mohamedtaha.store.EditCategory">

    <EditText
        android:id="@+id/ETNameCategory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="@string/name_category"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="@dimen/margingRight"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_creator="1"
        android:layout_marginEnd="@dimen/margingRight"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ETNaturalGategory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="@string/natural_category"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/ETNameCategory"
        android:layout_marginTop="@dimen/marginTopTen"
        app:layout_constraintTop_toBottomOf="@+id/ETNameCategory"
        />

    <EditText
        android:id="@+id/EtNotes"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="@string/notes"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/ETNaturalGategory"
        android:layout_marginTop="@dimen/marginTopTen"
        app:layout_constraintTop_toBottomOf="@+id/ETNaturalGategory"
        app:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="@dimen/margingRight"
       />

</android.support.constraint.ConstraintLayout>
