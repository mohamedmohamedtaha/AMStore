<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.imagine.mohamedtaha.store.dialog.DialogAddStore">
    <EditText
        android:id="@+id/ETTypeStore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="نوع المخزن"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="@dimen/margingRight"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="@dimen/margingRight"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ETNaturalStore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="طبيعةالمخزن"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/ETTypeStore"
        android:layout_marginTop="@dimen/marginTopTen"
        app:layout_constraintTop_toBottomOf="@+id/ETTypeStore"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
